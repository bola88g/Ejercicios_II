    "nums = [34, 1, 0, -23, -45,86]\n",
    "def dimePositivos(lista):\n",
    "    res = list(filter(lambda x: x >0, lista))\n",
    "    return res\n",
    "\n",
    "print(\"Los números en la lista original: \", nums,\"Los números positivos en la lista: \", dimePositivos(nums))"
